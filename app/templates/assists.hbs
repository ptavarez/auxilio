<h2>Auxes</h2>

{{#each model as |assist|}}
  {{#if assist.editable}}
    <ul>
      <li>
        {{#link-to 'assist' assist}}
          {{#if assist.service}}
            {{assist.service}}
          {{else}}
            Some Service
          {{/if}}
        {{/link-to}}
      </li>
    </ul>
  {{/if}}
{{/each}}

{{create-assist create='createAssist'}}
